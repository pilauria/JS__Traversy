{
  "name": "babel_webpack_starter",
  "version": "1.0.0",
  "description": "Starter pack for compiling ES6+ apps to ES5",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack serve --output-public-path=/build/  --mode development --progress --open --hot",
    "build": "webpack --mode production --progress",
    "serve": "webpack serve",
    "json:server": "json-server --watch api/db.json"
  },
  "author": "Brad Traversy",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "babel-loader": "^8.2.2",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.15.6",
    "webpack": "^5.54.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.3.0"
  }
}
